{    "type": "project",    "license": "proprietary",    "require": {        "php": "^7.1.10",        "ext-iconv": "*",        "sensio/framework-extra-bundle": "^5.1",        "symfony/console": "^3.4",        "symfony/debug": "^3.4",        "symfony/flex": "^1.0",        "symfony/framework-bundle": "^3.4",        "symfony/lts": "^3",        "symfony/yaml": "^3.4"    },    "require-dev": {        "symfony/dotenv": "^3.4"    },    "config": {        "preferred-install": {            "*": "dist"        },        "sort-packages": true    },    "autoload": {        "psr-4": {            "App\\": "src/"        }    },    "autoload-dev": {        "psr-4": {            "App\\Tests\\": "tests/"        }    },    "replace": {        "symfony/polyfill-apcu": "*",        "symfony/polyfill-php70": "*",        "symfony/polyfill-php56": "*"    },    "scripts": {        "auto-scripts": {            "cache:clear": "symfony-cmd",            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"        },        "post-install-cmd": [            "@auto-scripts"        ],        "post-update-cmd": [            "@auto-scripts"        ]    },    "conflict": {        "symfony/symfony": "*"    },    "extra": {        "symfony": {            "id": "01C16JE96NRS61B3QT52YD208Y",            "allow-contrib": false        }    }}